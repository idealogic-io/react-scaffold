    "@types/ua-parser-js": "^0.7.36",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "react-app-rewired": "^2.2.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.4.1",
    "typechain": "^8.3.2",
    "webpack": "^5.88.2"
  }
