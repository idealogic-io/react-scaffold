  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build-dev": "chmod +x ./scripts/build-dev.sh && ./scripts/build-dev.sh",
    "build-prod": "chmod +x ./scripts/build-prod.sh && ./scripts/build-prod.sh",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint -c .eslintrc.json --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "typechain": "typechain --target ethers-v5 --out-dir './src/configs/web3/abi/types' './src/configs/web3/abi/*.json'",
    "sitemap": "node ./sitemap-generator.mjs",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@storybook/preset-create-react-app": "^7.4.1",
    "@storybook/react-webpack5": "^7.4.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/lodash": "^4.17.0",
    "@types/react-dom": "^18.2.7",
    "@types/redux-logger": "^3.0.9",
